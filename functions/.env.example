# Ayrshare API Configuration
AYRSHARE_API_KEY=your-api-key-here
AYRSHARE_DOMAIN=your-domain-here
AYRSHARE_PROFILE_KEY=your-profile-key-here
AYRSHARE_PRIVATE_KEY=privatekeys/private.pem

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Brand Profile AI Configuration
OPENAI_API_KEY=your-openai-api-key-here

# Resend Email Configuration
RESEND_API_KEY=your-resend-api-key-here

# Subscription System Configuration
# =============================================

# Environment: development or production
NODE_ENV=development

# Whitelist for Testing (comma-separated emails)
# These emails can activate Ayrshare profiles without payment
TEST_ACCOUNT_EMAILS=your-email@example.com,dev@woozysocial.com

# Require active subscription to use Ayrshare features
# Set to false during initial development/testing
SUBSCRIPTION_REQUIRED=true

# Application URL (used for invitation links, webhooks, etc.)
APP_URL=http://localhost:5173
