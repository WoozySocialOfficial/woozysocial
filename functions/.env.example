# Ayrshare API Configuration
AYRSHARE_API_KEY=your-api-key-here
AYRSHARE_DOMAIN=your-domain-here
AYRSHARE_PROFILE_KEY=your-profile-key-here
AYRSHARE_PRIVATE_KEY=privatekeys/private.pem

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# AI Configuration (OpenAI)
OPENAI_API_KEY=your-openai-api-key-here

# Resend Email Configuration
RESEND_API_KEY=your-resend-api-key-here

# Subscription System Configuration
# =============================================

# Environment: development or production
NODE_ENV=development

# Whitelist for Testing (comma-separated emails)
# These emails can activate Ayrshare profiles without payment
TEST_ACCOUNT_EMAILS=your-email@example.com,dev@woozysocial.com

# Require active subscription to use Ayrshare features
# Set to false during initial development/testing
SUBSCRIPTION_REQUIRED=true

# Application URL (used for invitation links, webhooks, etc.)
APP_URL=http://localhost:5173

# Stripe Payment Configuration
# =============================================

# Stripe Secret Key (starts with sk_live_ or sk_test_)
STRIPE_SECRET_KEY=sk_test_xxx

# Stripe Webhook Secret (starts with whsec_)
# Get this from Stripe Dashboard > Developers > Webhooks
STRIPE_WEBHOOK_SECRET=whsec_xxx

# Stripe Price IDs for each tier
# Create these products/prices in Stripe Dashboard
STRIPE_PRICE_SOLO=price_xxx
STRIPE_PRICE_PRO=price_xxx
STRIPE_PRICE_PRO_PLUS=price_xxx
STRIPE_PRICE_AGENCY=price_xxx
STRIPE_PRICE_BRAND_BOLT=price_xxx

STRIPE_PRICE_SOLO_ANNUAL=price_xxx
STRIPE_PRICE_PRO_ANNUAL=price_xxx
STRIPE_PRICE_PRO_PLUS_ANNUAL=price_xxx
STRIPE_PRICE_AGENCY_ANNUAL=price_xxx
STRIPE_PRICE_BRAND_BOLT_ANNUAL=price_xxx